<div class="projects-one project-one__home-one">
    <div class="container">
        <div class="block-title text-center">
            <div class="block-title__image"></div><!-- /.block-title__image -->
            <h3>Categories</h3>
        </div><!-- /.block-title -->
        <div class="thm-swiper__slider swiper-container" data-swiper-options='{"spaceBetween": 0, "slidesPerView": 1, "loop": true, "slidesPerGroup": 1, "pagination": {
            "el": "#projects-one__swiper-pagination",
            "type": "bullets",
            "clickable": true
        },
        "breakpoints": {
            "0": {
                "spaceBetween": 0,
                "slidesPerView": 1,
                "slidesPerGroup": 1
            },
            "640": {
                "spaceBetween": 30,
                "slidesPerView": 2,
                "slidesPerGroup": 2
            },
            "992": {
                "spaceBetween": 30,
                "slidesPerView": 2,
                "slidesPerGroup": 2
            }
        }}'>
            <div class="swiper-wrapper">
                {{#each categories}}
                <div class="swiper-slide">
                    <div class="projects-one__single">
                        <img src="/public/images/projects/project-2-3.jpg" alt="">
                        <div class="projects-one__content">
                            <h3>{{this.name}}</h3>
                            <a href="/category/{{this.id}}" class="projects-one__button"><i
                                    class="agrikon-icon-right-arrow"></i></a><!-- /.projects-one__button -->
                        </div><!-- /.projects-one__content -->
                    </div><!-- /.projects-one__single -->
                </div><!-- /.swiper-slide -->
                {{/each}}
            </div><!-- /.swiper-wrapper -->
            <div class="swiper-pagination" id="projects-one__swiper-pagination"></div>
        </div><!-- /.swiper-container -->
    </div><!-- /.container -->
</div><!-- /.projects-one -->